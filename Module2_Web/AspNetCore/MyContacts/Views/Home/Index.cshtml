@{
    ViewData["Title"] = "Home Page";
}
@model Newtonsoft.Json.Linq.JArray
<h2>My Contacts</h2>
<div class="row">
    <div class="col-xs-12">
        <table class="table table-striped table-hover">
            <thead>
                <tr>
                    <th>Name</th>
                    <th>Email</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var contact in Model)
                {
                <tr>
                    <td>@contact.SelectToken("displayName")</td>
                    <td>@contact.SelectToken("emailAddresses").First.SelectToken("address")</td>
                </tr>
                }
            </tbody>
        </table>
    </div>
</div>
